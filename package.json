{
  "name": "api",
  "author": {
    "name": "Lucas de Souza",
    "url": "https://github.com/hexagon-0"
  },
  "packageManager": "yarn@4.3.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon -x ts-node src/index.ts",
    "test": "jest",
    "migrate:make": "knex --knexfile ./src/database/environment.ts migrate:make",
    "migrate:latest": "knex --knexfile ./src/database/environment.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./src/database/environment.ts migrate:rollback",
    "migrate:rollback-all": "knex --knexfile ./src/database/environment.ts migrate:rollback --all"
  },
  "dependencies": {
    "argon2": "^0.40.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.12.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/pg": "^8",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
